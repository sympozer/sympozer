    <NamedIndividual rdf:about="http://data.semanticweb.org/conference/eswc/2013/tracks/in-use-track">
        <rdf:type rdf:resource="&swc;TrackEvent"/>
        <rdfs:label>Track: In-Use Track</rdfs:label>
