    <NamedIndividual rdf:about="http://data.semanticweb.org/conference/eswc/2013/paper/eswc-2013/43">
        <rdf:type rdf:resource="&swrc;InProceedings"/>
        <rdfs:label>Predicting the Understandability of OWL Inferences</rdfs:label>
