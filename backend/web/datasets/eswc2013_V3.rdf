    <!-- http://data.semanticweb.org/conference/eswc/2013/talk/eswc-2013/199 -->

    <NamedIndividual rdf:about="http://data.semanticweb.org/conference/eswc/2013/talk/eswc-2013/199">
        <rdf:type rdf:resource="&swc;TalkEvent"/>
        <rdfs:label>Talk: Combining a co-occurrence-based and a semantic Measure for Entity Linking</rdfs:label>
        <swc:isSubEventOf rdf:resource="http://data.semanticweb.org/conference/eswc/2013/session/2-2-2"/>
