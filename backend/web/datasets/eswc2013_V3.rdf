    <NamedIndividual rdf:about="http://data.semanticweb.org/conference/eswc/2013/talk/eswc-2013/97">
        <rdf:type rdf:resource="&swc;TalkEvent"/>
        <rdfs:label>Talk: The Wisdom of the Audience: An Empirical Study of Social Semantics in Twitter Streams</rdfs:label>
